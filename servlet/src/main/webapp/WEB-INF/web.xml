<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>servlet1</servlet-name>
		<servlet-class>edu.home.app1.ServletLifeCycle</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet>
		<servlet-name>servlet2</servlet-name>
		<servlet-class>edu.home.app2.ServletApp2</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	
	<servlet>
		<servlet-name>servlet3</servlet-name>
		<servlet-class>edu.home.app3.ServletCommunication</servlet-class>
		<load-on-startup>3</load-on-startup>
	</servlet>
	
	<servlet>
		<servlet-name>servlet4</servlet-name>
		<servlet-class>edu.home.app3.ServletNetSalary</servlet-class>
		<load-on-startup>4</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>servlet1</servlet-name>
		<url-pattern>/app1</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>servlet2</servlet-name>
		<url-pattern>/app2</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>servlet3</servlet-name>
		<url-pattern>/app3</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>servlet4</servlet-name>
		<url-pattern>/net</url-pattern>
	</servlet-mapping>
</web-app>
